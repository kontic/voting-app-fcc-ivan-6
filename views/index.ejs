<!DOCTYPE html>
<html>
  <head>

    <% include partials/head %>

  </head>
  <body>
    
    <% include partials/header %>
      
    <div class="container">
      <div class="create_vote_container">
        
        <h2 class="w3-text-blue">All Polls</h2>
        
        <ul class="w3-ul l w3-hoverable">
          <% for( var i = 0; i < polls.length; i++ ) {%>
            <a href="polls/<%= polls[i].user_name %>/<%= polls[i].link_code %>">
            <li class="w3-display-container w3-hover-green w3-blue list-poll-item"><%= polls[i].poll_name %></li>
            </a>
          <% } %>
        </ul>
        
      </div>
    </div>
    
    <% include partials/footer %>
    
  </body>
</html>
